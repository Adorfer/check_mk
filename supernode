#!/bin/bash
#/usr/lib/check_mk_agent/local
export LANG=de_DE.UTF-8

## Supernode-status
if [ -f "/etc/supernode/cmk_supernode_status.txt" ]; then
  cat /etc/supernode/cmk_supernode_status.txt
 fi
 
## Supernode-Role-Swich-status
if [ -f "/etc/supernode/cmk_role_status.txt" ]; then
  cat /etc/supernode/cmk_role_status.txt
 fi
 

